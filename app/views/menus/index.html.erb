<%=form_for(@menu, :html=>{class:"dropzone", id: "menu-dropzone", multipart: true}) do %>
<% end %>
<%= link_to 'Done', menus_path(current_user) %>
<br>
<% @user.menus.each do |m| %>
  <%=image_tag m.menu.url(:thumb) %>
  <%= link_to "<span class='glyphicon glyphicon-trash' aria-hidden='true' </span>".html_safe,m, method: :delete %>
<% end %>